                                                while(perb == true){
                                                    System.out.print("Enter number of Extra Shampoo: ");
                                                    String pers = sc.next();
                                                    char perc = pers.charAt(0);
                                                    if(Character.isDigit(perc)){
                                                        double perd = Double.parseDouble(pers);
                                                        double solve = perpes*perd;
                                                        paymentSH = solve;
                                                        payment2 = payment2 + paymentSH;
                                                        System.out.print("\nDo you want to add more?\n1. Yes\n2. No\n");
                                                        perb = false;
                                                        fees1 = false;

                                                    }else{
                                                        System.out.println("Invalid Input.");
                                                        perb = true;
                                                        MainMenu = false;
                                                        SubMenu = false;
                                                        break;
                                                    }
                                                }
                                            }
                                            else{
                                                System.out.println("\nInvalid Input.");
                                            }
                                        }
                                    }
                                    else if(fc.equals("2")){
                                        payment = payment;
                                        System.out.println("\n===========================================================\n");
                                        System.out.println("Date: " + date + "\nTime: " + time);
                                        System.out.println("Thank You For Booking Mr/Ms. "+ roomName);
                                        System.out.println("You select room number "+ (roomNum) + " of Family Room" );
                                        System.out.println("You're overall Payment is Php " + payment);
                                        System.out.println("Your Room Code is " + int_code);
                                        System.out.println("\n===========================================================\n");

                                        boolean dp = true;
                                        while(dp == true){
                                            System.out.println("Need minimum 100 pesos for Deposit: ");
                                            String dpmc = sc.next();
                                            
                                            try {
                                                double mc = Double.valueOf(dpmc);
                                                boolean low = true;
                                                while(low == true){
                                                    
                                                    if(mc >= 100){
                                                        if (mc <= payment){
                                                            double dpmb = payment - mc ;
                                                            System.out.println("\n-----------------------------------------------------------\n");
                                                            System.out.println("\nYour remaining balance: Php "+ dpmb);
                                                            System.out.println("\n-----------------------------------------------------------\n");
                                                            System.out.println("\n===========================================================\n");
                                                            dp = false;
                                                            fees = false;
                                                            rmtp = false;
                                                            aps = false;
                                                            rm = false;
                                                            MainMenu = true;
                                                            SubMenu = true;
                                                            break;
                                                        }
                                                        else{
                                                            double dpmb = mc - payment ;
                                                            System.out.println("\n-----------------------------------------------------------\n");
                                                            System.out.println("\nYou have remaning exchange: Php "+ dpmb);
                                                            System.out.println("\n-----------------------------------------------------------\n");
                                                            System.out.println("\n===========================================================\n");
                                                            dp = false;
                                                            fees = false;
                                                            rmtp = false;
                                                            aps = false;
                                                            rm = false;
                                                            MainMenu = true;
                                                            SubMenu = true;
                                                            break;
                                                        }
                                                    }else{
                                                        System.out.println("Invalid Payment. It's lower than 100.");
                                                        low = false;
                                                        dp = true;
                                                    }
                                                }
                                                
                                            } catch(NumberFormatException e) {
                                                System.out.println("\nInvalid Payment. Put Numbers Only.\n");
                                                dp = true;
                                            }
                                        }
                                    }
                                    else{
                                        System.out.println("\nInvalid Input.");
                                    }
                                }
                            }
                            else{
                                System.out.println("\nInvalid Input.");
                            }
                        }
                    }
                }
            }else{
                System.out.println("Invalid Input.");
                rm = true;
                MainMenu = false;
                SubMenu = false;
            }
        }
    }
